<?xml version="1.0" ?>

<launch>

  <!-- Arguments -->
  <arg name="robot_name"/>  
  <arg name="tf_robot"/> 
  <arg name="cmd_vel_topic" default="/cmd_vel" />
  <arg name="odom_topic" default="odom" />
  <arg name="move_forward_only" default="false"/>

  <!--- Run AMCL -->
  <include file="$(find medibotv4)/launch/navigation/multirobot_amcl.launch">
    <arg name="use_map_topic"             value="true"/>
    <arg name="global_frame_id" value="/map"/>
    <arg name="odom_frame_id"   value="$(arg tf_robot)/odom"/>
    <arg name="base_frame_id"   value="$(arg tf_robot)/base_link"/>
    
  </include>

  <include file="$(find medibotv4)/launch/navigation/move_base_ns.launch">
    
  </include>

  
</launch>